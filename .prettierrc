{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "printWidth": 79,
    "quoteProps": "consistent",

    "overrides": [
        {
            "files": "*.py",
            "options": {
                "tabWidth": 4
            }
        },
        {
            "files": "*.js",
            "options": {
                "tabWidth": 4,
                "semi": true
            }
        }
    ]
}
